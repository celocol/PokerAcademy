# Testnet Configuration for Alfajores
# Copy this file to .env.local for local development

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# CeloScan API key for contract verification
CELOSCAN_API_KEY=your_celoscan_api_key_here

# Contract address (will be filled after deployment to testnet)
CONTRACT_ADDRESS=

# Next.js environment variables for testnet
NEXT_PUBLIC_CONTRACT_ADDRESS=your_deployed_contract_address_on_alfajores
NEXT_PUBLIC_NETWORK_ID=44787
NEXT_PUBLIC_NETWORK_NAME=Alfajores

# Testnet RPC URL
NEXT_PUBLIC_RPC_URL=https://alfajores-forno.celo-testnet.org

# Testnet Explorer
NEXT_PUBLIC_EXPLORER_URL=https://alfajores.celoscan.io 